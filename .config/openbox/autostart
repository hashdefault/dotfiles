xset b off &
xrandr --output HDMI1  --mode  1920x1080  --pos 0x0 --rotate normal --output eDP1  --mode 1366x768 --pos 1920x100  --rotate normal  &
nitrogen --restore &
sleep 1 &
plank -n dock1 &
sleep 1 &
plank -n dock2 &
picom &
killall polybar; ~/.config/polybar/launch.sh &
setxkbmap us altgr-intl &
