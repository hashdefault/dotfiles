xset b off &
xrandr --output DP-1 --mode 1920x1080 --pos 0x0 --rotate normal --output HDMI-1  --mode  1920x1080 --pos 1920x0 --rotate normal &
xrandr --output HDMI-1 --gamma 1.0:0.88:0.56 --brightness 0.95 &
xrandr --output DP-1 --gamma 1.0:0.88:0.56 --brightness 0.95 &
plank -n dock1 &
plank -n dock2 &
nitrogen --restore &
picom &
killall polybar; ~/.config/polybar/launch.sh &
setxkbmap us altgr-intl &
